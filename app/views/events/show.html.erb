<p>Name: <%= @event.name%> </p>
<p>Date: <%= @event.date %></p>

<% @timeslots.each do |timeslot| %>
    <p>
        <%= timeslot.time.strftime('%l:%M %P') %>: <a href="#">Claim</a>
    </p>
<% end %>

<p>
    <%= link_to "Add Timeslots", new_timeslot_path(event_id: @event.id)%>
</p>
<p>
    <%= link_to "Homepage", events_path %>
</p>
