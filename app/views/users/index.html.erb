<h1> You made it to the index page for users! </h1>


<% @user.each do |user| %>
    <tr class="header">
    <td><%= link_to user.full_name,user_path(user) %>
    <td> Total Approved Hours: <%= user.total_approved_hours %>
    <td> Total Unapproved Hours: <%= user.total_unapproved_hours %>
    <td> User Role: <%= user.user_role.name %>
    </td>
</tr>
<% end %>